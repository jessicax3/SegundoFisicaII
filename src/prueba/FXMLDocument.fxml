<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>                          

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="490.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml" fx:controller="prueba.FXMLDocumentController">
   <children>
      <ImageView fitHeight="503.0" fitWidth="714.0" layoutX="-3.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imagenes/fondo.jpg" />
         </image>
      </ImageView>
      <Pane fx:id="paneview" prefHeight="494.0" prefWidth="707.0">
         <children>
            <HBox fx:id="botones" layoutX="226.0" layoutY="377.0" prefHeight="64.0" prefWidth="255.0">
               <children>
                  <Button fx:id="btnSalir" mnemonicParsing="false" onAction="#botonSalir" prefHeight="25.0" prefWidth="126.0" style="-fx-background-color: #660000;" text="Salir">
                     <effect>
                        <DropShadow blurType="TWO_PASS_BOX" color="#b90b0b" height="30.0" radius="16.6375" spread="0.28" width="38.55" />
                     </effect>
                  </Button>
                  <Button fx:id="btnIniciar" mnemonicParsing="false" onAction="#botonContinuar" prefHeight="25.0" prefWidth="126.0" style="-fx-background-color: #006666;" text="Iniciar" textAlignment="CENTER">
                     <effect>
                        <DropShadow blurType="TWO_PASS_BOX" color="#1b6d6f" height="22.36" radius="14.725" spread="0.34" width="38.54" />
                     </effect>
                  </Button>
               </children>
            </HBox>
         </children></Pane>
   </children>
</AnchorPane>
